<h1>Addresses#show</h1>
<p>Find me in app/views/addresses/show.html.erb</p>

<h3>Address:  <%= @address.address %></h3>
<h3>City: <%= @address.city %></h3>
<h3>State:  <%= @address.state %></h3>
<h3>Zip_Code: <%= @address.zip_code %></h3>
<h3>Country:  <%= @address.country %></h3>
<h3>Telephone:  <%= @address.telephone %></h3>
<h3>Address_Type: <%= @address.address_type %></h3>
<%= link_to 'Edit', edit_address_path(@address) %>
<%= link_to 'Home', addresses_path %>
<%= link_to 'delete', address_path(@address), data: {turbo_method: :delete, turbo_confirm: 'Are you sure?' } %>